{
    "name": "Wagaku Test",
    "tokenColors": [
        {
            "name": "Source",
            "scope": [
                "source",
                "entity.name.function.call.initializer"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#919985"
            }
        },
        {
            "name": "Punctuation and Operator",
            "scope": [
                "punctuation",
                "keyword.operator",
                "entity.name.function.operator",
                "storage.modifier.pointer",
                "storage.modifier.reference",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#D2D4CE"
            }
        },
        {
            "name": "Keyword and Macro",
            "scope": [
                "keyword",
                "keyword.operator.cast",
                "keyword.operator.delete",
                "keyword.operator.new",
                "storage",
                "constant.language",
                "variable.language.this",
                "entity.other.attribute",
                "meta.preprocessor",
                "punctuation.definition.directive",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#69D4E2"
            }
        },
        {
            "name": "Type: class and struct",
            "settings": {
                "foreground": "#ED755A"
            },
            "scope": [
                "entity.name.type"
            ]
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#e2c200"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "keyword.other.unit",
                "keyword.other.unit.suffix",
                "string",
                "constant.character",
                "constant.other.placeholder",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#D5C2E5"
            }
        },
        {
            "name": "json Property",
            "scope": [
                "support.type",
                "punctuation.support.type"
            ],
            "settings": {
                "foreground": "#e2c200"
            }
        },
        {
            "name": "Markdown Header",
            "scope": [
                "entity.name.section",
                "punctuation.definition.heading"
            ],
            "settings": {
                "foreground": "#e2c200"
            }
        },
        {
            "name": "Markdown List Punctuation",
            "scope": [
                "meta.paragraph"
            ],
            "settings": {
                "foreground": "#D5C2E5"
            }
        }
    ],
    "colors": {
        "foreground": "#E0E2DF",
        "errorForeground": "#FF94A4",
        "focusBorder": "#919985",
        "selection.background": "#960E27e8",
        "titleBar.activeBackground": "#960E27",
        "titleBar.activeForeground": "#E0E2DF",
        "button.background": "#960E27",
        "list.dropBackground": "#161a1180",
        "list.hoverBackground": "#960E27",
        "list.focusBackground": "#960E27",
        "editorGroupHeader.tabsBackground": "#2B3022",
        "editorGroupHeader.noTabsBackground": "#2B3022",
        "tab.activeBackground": "#161a11",
        "tab.activeForeground": "#E0E2DF",
        "tab.inactiveBackground": "#2B3022",
        "tab.inactiveForeground": "#E0E2DF",
        "tab.border": "#2B3022",
        "editor.background": "#161a11",
        "editorCursor.foreground": "#E0E2DF",
        "editorBracketMatch.background": "#960E2780",
        "editorBracketMatch.border": "#CD1335",
        "editorLineNumber.foreground": "#960E27",
