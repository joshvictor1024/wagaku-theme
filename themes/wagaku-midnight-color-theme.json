{
    "name": "Wagaku Midnight",
    "tokenColors": [
        {
            "name": "Source",
            "scope": [
                "source",
                "entity.name.function.call.initializer",
                "entity.name.type.parameter.cpp",
                "entity.name.scope-resolution",
                "variable",
                "meta.paragraph",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        },
        {
            "name": "Italic",
            "scope": [
                "markup.italic",
                "punctuation.definition.italic"
            ],
            "settings": {
                "foreground": "#E5D7D8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Bold",
            "scope": [
                "markup.bold",
                "punctuation.definition.bold"
            ],
            "settings": {
                "foreground": "#E5D7D8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#919985"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "storage.modifier.pointer",
                "storage.modifier.reference",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        },
        {
            "name": "Overloaded Operator",
            "scope": [
                "entity.name.function.operator"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        },
        {
            "name": "Parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        },
        {
            "name": "Keyword, Storage and Macro",
            "scope": [
                "keyword",
                "keyword.operator.cast",
                "keyword.operator.delete",
                "keyword.operator.new",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.alignas",
                "constant.language",
                "variable.language.this",
                "entity.other.attribute",
                "storage",
                "meta.preprocessor",
                "punctuation.definition.directive",
                "entity.name.function.preprocessor",
                "variable.language.special.self",
                "variable.parameter.function.language.special.self",
                "keyword.operator.expression.typeof",
                "keyword.operator.expression.in"
            ],
            "settings": {
                "foreground": "#69D4E2"
            }
        },
        {
            "name": "Type",
            "scope": [
                "entity.name.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#ED755A"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#E2C200"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "keyword.other.unit",
                "keyword.other.unit.suffix",
                "string",
                "constant.character",
                "constant.other",
                "punctuation.definition.string",
                "markup.inline.raw.string",
                "punctuation.definition.raw"
            ],
            "settings": {
                "foreground": "#d1bae5"
            }
        },
        {
            "name": "Python decorators",
            "scope": [
                "punctuation.definition.decorator"
            ],
            "settings": {
                "foreground": "#ED755A"
            }
        },
        {
            "name": "Json Property",
            "scope": [
                "support.type.property-name",
                "punctuation.support.type.property-name"
            ],
            "settings": {
                "foreground": "#E2C200"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": [
                "punctuation.definition.heading",
                "markup.heading.setext"
            ],
            "settings": {
                "foreground": "#E2C200"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#E2C200"
            }
        },
        {
            "name": "Markdown Separator",
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "foreground": "#E5D7D8"
            }
        }
    ],
