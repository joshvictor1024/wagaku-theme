{
	"name": "theme-wagaku",
	"displayName": "Theme Wagaku",
	"version": "0.4.2",
	"categories": [
		"Themes"
	],
	"description": "A minimally colorful theme inspired by the album art of Yoen Waso Emaki",
	"contributes": {
		"themes": [
			{
				"label": "Wagaku Midnight",
				"uiTheme": "vs-dark",
				"path": "./themes/wagaku-midnight-color-theme.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Theme Wagaku",
			"properties": {
				"themeWagaku.operatorColor": {
					"type": "string",
					"default": "Punctuation",
					"enum": [
						"Punctuation",
						"Keyword",
						"Function"
					],
					"description": "Set textmate color of operators",
					"scope": "window"
				},
				"themeWagaku.overloadedOperatorColor": {
					"type": "string",
					"default": "Punctuation",
					"enum": [
						"Punctuation",
						"Keyword",
						"Function"
					],
					"description": "Set textmate color of overloaded operators",
					"scope": "window"
				}
			}
		}
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./extension.js",
	"author": "Josh Chuang",
	"publisher": "josh-chuang",
	"license": "MIT",
	"engines": {
		"vscode": "^1.54.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/joshvictor1024/wagaku-theme"
	},
	"galleryBanner": {
		"color": "161a11",
		"theme": "dark"
	},
	"icon": "icon.png",
	"keywords": [
		"oriental",
		"minimal",
		"colorful",
		"simple",
		"clean",
		"red",
		"cpp",
		"C++"
	],
	"dependencies": {
		"vsce": "^1.84.0"
	},
	"devDependencies": {
		"node": "^12.18.3",
		"uglifyjs": "^3.13.0"
	},
	"__metadata": {
		"id": "ddfcd9a4-e476-4090-984c-30da74b216c9",
		"publisherDisplayName": "Josh Chuang",
		"publisherId": "389aaa02-136a-45a5-96cf-1ebeea57dcef"
	}
}